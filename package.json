{
  "name": "nspass-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "proto:generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./app/types/generated --ts_proto_opt=stringEnums=true,snakeToCamel=true,useOptionals=all,onlyTypes=true --proto_path=./proto $(find proto -name '*.proto')",
    "proto:watch": "chokidar \"proto/**/*.proto\" -c \"npm run proto:generate\""
  },
  "dependencies": {
    "@ant-design/charts": "^2.3.0",
    "@ant-design/graphs": "^2.1.0",
    "@ant-design/plots": "^2.4.0",
    "@ant-design/pro-components": "^2.8.7",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@types/bcryptjs": "^2.4.6",
    "antd": "^5.25.1",
    "bcryptjs": "^3.0.2",
    "leaflet": "^1.9.4",
    "next": "15.3.2",
    "pigeon-maps": "^0.22.1",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-leaflet": "^3.0.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "msw": "^2.9.0",
    "tailwindcss": "^4",
    "ts-proto": "^2.7.2",
    "typescript": "^5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
