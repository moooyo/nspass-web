{
  "name": "nspass-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run proto:check && next dev",
    "dev:fast": "next dev",
    "dev:clean": "npm run clean && npm run proto:clean && npm run proto:generate && next dev",
    "dev:proto": "npm run proto:generate && next dev",
    "dev:turbo": "npm run proto:check && next dev --turbopack",
    "build": "npm run proto:clean && npm run proto:generate && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:cloudflare": "npm run proto:clean && npm run proto:generate && npx @cloudflare/next-on-pages",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache && npm cache clean --force",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "msw:init": "npx msw init public/ --save",
    "msw:clean": "rm -f public/mockServiceWorker.js",
    "msw:reset": "npm run msw:clean && npm run msw:init",
    "msw:force-reset": "npm run msw:reset && echo '🔄 请刷新浏览器页面以清理Service Worker缓存'",
    "fix:msw": "npm run msw:reset && npm run dev:fast",
    "proto:clean": "rm -rf ./app/types/generated",
    "proto:generate": "mkdir -p ./app/types/generated && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./app/types/generated --ts_proto_opt=stringEnums=true,snakeToCamel=true,useOptionals=all,onlyTypes=true --proto_path=./proto $(find proto -name '*.proto')",
    "proto:watch": "chokidar \"proto/**/*.proto\" -c \"npm run proto:generate\"",
    "proto:dev": "npm run proto:clean && npm run proto:generate && npm run proto:watch",
    "proto:check": "[ -d ./app/types/generated ] && [ -n \"$(ls -A ./app/types/generated 2>/dev/null)\" ] || npm run proto:generate",
    "predev": "npm run proto:check",
    "prebuild": "npm run type-check"
  },
  "dependencies": {
    "@ant-design/charts": "^2.3.0",
    "@ant-design/graphs": "^2.1.0",
    "@ant-design/plots": "^2.4.0",
    "@ant-design/pro-components": "^2.8.7",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@types/bcryptjs": "^2.4.6",
    "antd": "^5.25.1",
    "bcryptjs": "^3.0.2",
    "country-flag-emoji": "^1.0.3",
    "leaflet": "^1.9.4",
    "next": "15.3.2",
    "pigeon-maps": "^0.22.1",
    "react": "^19.0.0",
    "react-country-flag": "^3.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-simple-maps": "^3.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-leaflet": "^3.0.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "msw": "^2.9.0",
    "tailwindcss": "^4",
    "ts-proto": "^2.7.2",
    "typescript": "^5",
    "wrangler": "^3.93.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
