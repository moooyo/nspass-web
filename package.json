{
  "name": "nspass-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development VITE_API_BASE_URL=http://localhost:8080 vite",
    "dev:watch": "npm run proto:check && vite --watch",
    "build": "NODE_ENV=production VITE_API_BASE_URL=https://api.nspass.xforward.de npm run proto:check && vite build && cp src/favicon.ico dist/favicon.ico",
    "build:dev": "NODE_ENV=development VITE_API_BASE_URL=https://api.nspass.xforward.de npm run proto:check && vite build && cp src/favicon.ico dist/favicon.ico",
    "preview": "npm run build && vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "proto:clean": "rm -rf ./types/generated",
    "proto:generate": "./scripts/create-fallback-types.sh",
    "proto:check": "[ -d ./types/generated ] && [ -n \"$(ls -A ./types/generated 2>/dev/null)\" ] || npm run proto:generate",
    "postinstall": "./scripts/install-protoc.sh",
    "worker:deploy": "npm run build && NODE_TLS_REJECT_UNAUTHORIZED=0 npx wrangler deploy",
    "worker:dev": "npm run build && NODE_TLS_REJECT_UNAUTHORIZED=0 npx wrangler dev",
    "worker:tail": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx wrangler tail nspass-web --format pretty",
    "clean": "rm -rf dist out node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "health-check": "npm run lint && npm run type-check",
    "analyze": "npm run build && node scripts/build-optimize.js",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist",
    "perf": "npm run build && npm run preview -- --open",
    "dev:debug": "NODE_ENV=development DEBUG=* vite",
    "test:build": "npm run build && node -e \"console.log('Build test passed')\"",
    "prepare": "npm run proto:check"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tanstack/react-table": "^8.20.5",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "country-flag-emoji": "^1.0.3",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.468.0",
    "react": "^19.0.0",
    "react-country-flag": "^3.1.0",
    "react-day-picker": "^9.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.13.3",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@hookform/resolvers": "^3.10.0",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^22.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.3.0",
    "msw": "^2.6.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "ts-proto": "^2.7.5",
    "typescript": "^5",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "wrangler": "^4.23.0",
    "zod": "^3.24.1"
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "overrides": {
    "cookie": "^0.7.0",
    "esbuild": "^0.25.5",
    "path-to-regexp": "^8.0.0",
    "undici": "^6.21.0"
  }
}
